{
    "configurations": [
        
        {
            "type": "spring-boot",
            "name": "KetchappApplication",
            "mainClass": "com.alessandra_alessandro.ketchapp.KetchappApplication",
            "module": "ketchapp.main",
            "options": ["-XX:TieredStopAtLevel=1", "-Dspring.output.ansi.enabled=always", "-Dcom.sun.management.jmxremote", "-Dspring.jmx.enabled=true", "-Dspring.liveBeansView.mbeanDomain", "-Dspring.application.admin.enabled=true", "-Dmanagement.endpoints.jmx.exposure.include=*", "-Dfile.encoding=UTF-8", "-Dsun.stdout.encoding=UTF-8", "-Dsun.stderr.encoding=UTF-8"],
            "activeProfiles": [""],
            "workingDir": "$PROJECT_DIR$",
            "dependsOn": ["Build KetchApp-API"]
        },
        {
            "type": "gradle",
            "name": "Build KetchApp-API",
            "tasks": [":classes"],
            "initScripts": {
                "flmapper": "ext.mapPath = { path -> path }",
                "Build KetchApp-API": "System.setProperty('org.gradle.java.compile-classpath-packaging', 'true')\n"
            },
            "workingDir": "$PROJECT_DIR$"
        }
    ]
}